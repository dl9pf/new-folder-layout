+	.num_hw_mod_clks = 12 * 32,
+
+	/* Critical Module Clocks */
+	.crit_mod_clks = r8a7798_crit_mod_clks,
+	.num_crit_mod_clks = ARRAY_SIZE(r8a7798_crit_mod_clks),
+
+	/* Callbacks */
+	.init = r8a7798_cpg_mssr_init,
+	.cpg_clk_register = rcar_gen3_cpg_clk_register,
+};
